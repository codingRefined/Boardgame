pipeline {
    agent {
  label 'slave'
}
    

    tools {
  jdk 'jdk17'
  maven 'mavenLatest'
}
    stages {
        stage('git') {
            steps {
               git branch: 'main', url: 'https://github.com/codingRefined/Boardgame.git'
            }
        }
        
         stage('compile') {
            steps {
             sh 'mvn compile'
            }
        }
        
    }
   post{
        success{
            build job : 'multibranchProject/main'
        }
   }
}
